          }
        case 127: break;
        case 45: 
          { return token(yytext(),"TKN_LIMPIAR",yyline,yycolumn);
          }
        case 128: break;
        case 12: 
          { return token(yytext(),"OP_NOT",yyline,yycolumn);
          }
        case 129: break;
        case 29: 
          { return token(yytext(),"OP_DIF",yyline,yycolumn);
          }
        case 130: break;
        case 69: 
          { return token(yytext(),"TKN_FIN_MIENTRAS",yyline,yycolumn);
          }
        case 131: break;
        case 74: 
          { return token(yytext(),"TKN_BALANCEAR_REACCION",yyline,yycolumn);
          }
        case 132: break;
        case 5: 
          { return token(yytext(), "ERROR", yyline, yycolumn);
          }
        case 133: break;
        case 24: 
          { return token(yytext(),"PUNTO_COMA",yyline,yycolumn);
          }
        case 134: break;
        case 18: 
          { return token(yytext(),"LLAVE_ABRE",yyline,yycolumn);
          }
        case 135: break;
        case 34: 
          { return token(yytext(),"TKN_PESO",yyline,yycolumn);
          }
        case 136: break;
        case 37: 
          { return token(yytext(),"TKN_HACER",yyline,yycolumn);
          }
        case 137: break;
        case 8: 
          { return token(yytext(),"OP_MAS",yyline,yycolumn);
          }
        case 138: break;
        case 3: 
          { /* ignorar */
          }
        case 139: break;
        case 59: 
          { return token(yytext(),"TKN_CALENTAR",yyline,yycolumn);
          }
        case 140: break;
        case 6: 
          { return token(yytext(),"OP_MENOS",yyline,yycolumn);
          }
        case 141: break;
        case 44: 
          { return token(yytext(),"TKN_MEZCLAR",yyline,yycolumn);
          }
        case 142: break;
        case 65: 
          { return token(yytext(),"TKN_MEDIR_TEMPERATURA",yyline,yycolumn);
          }
        case 143: break;
        case 13: 
          { return token(yytext(),"OP_MENOR",yyline,yycolumn);
          }
        case 144: break;
        case 51: 
          { return token(yytext(),"TKN_MIENTRAS",yyline,yycolumn);
          }
        case 145: break;
        case 39: 
          { return token(yytext(),"TKN_ACUOSO",yyline,yycolumn);
          }
        case 146: break;
        case 35: 
          { return token(yytext(),"TKN_MOLES",yyline,yycolumn);
          }
        case 147: break;
        case 41: 
          { return token(yytext(),"TKN_FIN_SI",yyline,yycolumn);
          }
        case 148: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            return null;
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
